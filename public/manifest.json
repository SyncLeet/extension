{
  "manifest_version": 3,

  "name": "SyncLeet",
  "description": "ðŸŽ‰ Synchronize your LeetCode submissions to GitHub. Happy Coding! ðŸš€",
  "version": "1.0",
  "action": {
    "default_icon": "logo.png"
  },

  "permissions": ["webRequest", "storage", "identity", "notifications"],
  "host_permissions": ["https://leetcode.com/*", "https://github.com/*"],

  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["foreground.js"]
    }
  ],

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "key": ""
}
