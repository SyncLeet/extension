{
  "manifest_version": 3,

  "name": "SyncLeet",
  "description": "Synchronize your LeetCode submissions to GitHub. Happy Coding! ðŸš€",
  "version": "2.0",
  "action": {
    "default_icon": "asset/image/logox48.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "asset/image/logox16.png",
    "48": "asset/image/logox48.png",
    "128": "asset/image/logox128.png"
  },

  "permissions": ["webRequest", "storage", "identity", "notifications"],
  "host_permissions": ["https://leetcode.com/*", "https://github.com/*"],

  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["script/foreground.js"]
    }
  ],

  "background": {
    "service_worker": "script/background.js",
    "type": "module"
  },

  "key": ""
}
